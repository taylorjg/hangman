{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "mocha": true,
        "jest": true
    },
    "rules": {
        "semi": [
            2,
            "always"
        ],
        "no-console": 0
    }
}